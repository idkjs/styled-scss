{
  "name": "styled-scss",
  "version": "0.0.1",
  "description": "A Sass to React compiler",
  "bin": {
    "styled": "./cli.js"
  },
  "files": [
    "cli.js",
    "index.js"
  ],
  "scripts": {
    "test": "jest",
    "local": "node src/cli.bs.js",
    "build:js": "bsb -clean-world -make-world",
    "build:cli": "rollup -c rollup/rollup.config.cli.js",
    "build:api": "rollup -c rollup/rollup.config.api.js",
    "build:web": "rollup -c rollup/rollup.config.web.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rrdelaney/Backsass.git"
  },
  "keywords": [
    "sass",
    "css",
    "css-in-js",
    "react",
    "reactjs",
    "react-css",
    "reason",
    "reasonml",
    "bucklescript",
    "reason-css"
  ],
  "author": "Ryan Delaney <rrdelaney@outlook.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rrdelaney/Backsass/issues"
  },
  "homepage": "https://github.com/rrdelaney/Backsass#readme",
  "dependencies": {
    "@babel/core": "^7.0.0-beta.49",
    "@babel/generator": "^7.0.0-beta.49",
    "@babel/parser": "^7.0.0-beta.48",
    "@babel/traverse": "^7.0.0-beta.49",
    "@babel/types": "^7.0.0-beta.49",
    "babel-plugin-emotion": "^9.1.2",
    "chalk": "^2.4.1",
    "commander": "^2.15.1",
    "fb-watchman": "^2.0.0",
    "postcss": "^6.0.22",
    "prettier": "^1.13.3",
    "reasonably-typed": "^2.0.0-beta4",
    "sass": "^1.5.1"
  },
  "devDependencies": {
    "@babel/standalone": "^7.0.0-beta.49",
    "@glennsl/bs-jest": "^0.4.2",
    "@glennsl/bs-json": "^2.0.0",
    "bs-platform": "^3.1.5",
    "jest": "^23.1.0",
    "rollup": "^0.59.4",
    "rollup-plugin-alias": "^1.4.0",
    "rollup-plugin-commonjs": "^9.1.3",
    "rollup-plugin-json": "^3.0.0",
    "rollup-plugin-node-resolve": "^3.3.0"
  },
  "jest": {
    "testMatch": [
      "<rootDir>/src/*_test.bs.js"
    ]
  }
}
